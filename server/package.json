{
  "name": "server",
  "version": "0.12.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon -L --exec tsx src/index.ts",
    "reset": "node src/resetDB.js",
    "docs": "jsdoc -c jsdoc.json",
    "docs-dev": "concurrently --kill-others \"live-server --wait=1000 dist/docs\" \"nodemon --exec npm run docs\"",
    "build": "npm run clean && npm run lint && npm run build:ts",
    "build:ts": "tsc --project compile.tsconfig.json && tsc-alias --project compile.tsconfig.json && tscp --project compile.tsconfig.json",
    "test": "npm run test:prep && npm run test:unit",
    "test:prep": "npm run lint && tsc --noEmit",
    "test:unit": "jest",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "format-check": "prettier --check .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreasnicklaus/cheer-choreo-tool.git"
  },
  "author": "Andreas Nicklaus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreasnicklaus/cheer-choreo-tool/issues"
  },
  "homepage": "https://www.choreo-planer.de",
  "dependencies": {
    "@logtail/node": "^0.5.6",
    "@logtail/winston": "^0.5.6",
    "@sequelize/postgres": "^7.0.0-alpha.47",
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.1",
    "clean-jsdoc-theme": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "eslint": "^9.39.1",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^8.2.1",
    "express-robots-txt": "^1.0.0",
    "express-status-monitor": "^1.3.4",
    "fuse.js": "^7.1.0",
    "globals": "^16.5.0",
    "helmet": "^8.1.0",
    "i18n": "^0.15.3",
    "jsdoc": "^4.0.5",
    "jsonc.min": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "p-queue": "^9.0.1",
    "permissions-policy": "^0.6.0",
    "pg": "^8.16.3",
    "rimraf": "^6.1.2",
    "sequelize": "^6.37.7",
    "serve-favicon": "^2.5.1",
    "swagger-docs": "^0.0.0-alpha2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-cp": "^0.1.9",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@sequelize/sqlite3": "^7.0.0-alpha.47",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/sequelize": "^6.12.0",
    "concurrently": "^9.2.1",
    "jest-github-actions-reporter": "^1.0.3",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0"
  },
  "overrides": {
    "express-robots-txt": {
      "express": "^5"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "./dist"
    ],
    "ext": "ts,json,js"
  }
}
