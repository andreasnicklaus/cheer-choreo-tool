{
  "name": "choreo-tool",
  "version": "0.11.0",
  "private": true,
  "scripts": {
    "serve": "npm run icons && vue-cli-service serve",
    "build": "npm run icons && npm run docs && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "icons": "vue-asset-generate -a ./public/Icon.png -b #FFFFFF",
    "sitemap": "vue-cli-service sitemap",
    "docs": "jsdoc -c jsdoc.json",
    "docs:watch": "concurrently --kill-others \"live-server ./public/docs\" \"nodemon --exec npm run docs\"",
    "docs:update": "nodemon --exec npm run docs",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e:pretest": "tsc --noEmit",
    "test:e2e": "npm run test:e2e:pretest && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:unit": "jest"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/polyfill": "^7.11.5",
    "@dreysolano/prerender-spa-plugin": "^1.0.3",
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffmpeg/util": "^0.12.1",
    "@logtail/browser": "^0.5.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "axios": "^1.7.7",
    "axios-cache-interceptor": "^1.6.0",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "caniuse-lite": "^1.0.30001737",
    "clean-jsdoc-theme": "^4.3.0",
    "core-js": "^3.8.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "gsap": "^3.12.5",
    "javascript-time-ago": "^2.5.11",
    "jsdoc": "^4.0.4",
    "jsdoc-vuejs": "^4.0.0",
    "mutationobserver-shim": "^0.3.7",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "register-service-worker": "^1.7.2",
    "sass-loader": "^12.0.0",
    "vue": "^2.6.14",
    "vue-cli-plugin-bootstrap-vue": "~0.8.2",
    "vue-cli-plugin-sitemap": "~2.3.0",
    "vue-cookie": "^1.1.4",
    "vue-flag-icon": "^2.1.0",
    "vue-html2pdf": "^1.8.0",
    "vue-i18n": "^8.28.2",
    "vue-markdown-v2": "^0.1.7",
    "vue-matomo": "^4.2.0",
    "vue-meta": "^2.4.0",
    "vue-pwa-asset-generator": "^1.7.3",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@eslint/js": "^9.30.1",
    "@estruyf/github-actions-reporter": "^1.10.0",
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.1",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.30.1",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.10",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "serve": "^14.2.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vue-template-compiler": "^2.6.14"
  },
  "nodemonConfig": {
    "ignore": [
      "./public/docs"
    ],
    "ext": "vue,js,md"
  }
}
